{
  "connection": "default",
  "collectionName": "offer",
  "info": {
    "name": "offer",
    "description": "Offer content Type based on @schema.org"
  },
  "options": {
    "timestamps": [
      "createdAt",
      "updatedAt"
    ]
  },
  "attributes": {
    "serialNumber": {
      "type": "string"
    },
    "validFrom": {
      "type": "date"
    },
    "validTo": {
      "type": "date"
    },
    "advanceBookingRequirement": {
      "type": "boolean"
    },
    "image": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "gtin": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "Real_Price": {
      "type": "decimal"
    },
    "acceptedPaymentMethod": {
      "type": "string"
    },
    "Fit_Coin_Price": {
      "type": "integer"
    },
    "One_Time_Offer": {
      "type": "boolean"
    },
    "BusinessName": {
      "model": "businesses"
    }
  }
}